{
"name": "workshop/emergify",
"description": "This extension deals as an example site package to show
how we create one.",
"type": "typo3-cms-extension",
"license": [
"GPL-2.0-or-later"
],
"require": {
"typo3/cms-core": "^13.0"
},
"autoload": {
"psr-4": {
"Workshop\\SitePackage\\": "Classes/"
}
},
"extra": {
"typo3/cms": {
"extension-key": "emergify"
}
},
"version": "1.0.0"
},
"config": {
"bin-dir": ".",
},
"repositories": {
"local": {
"type": "path",
"url": "./packages/*"
}
}
